{
  "version": 2,
  "name": "pickbazar-api",
  "builds": [
    {
      "src": "./server.admin.ts",
      "use": "@now/node-server"
    },
    {
      "src": "./server.app.ts",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/admin/graphql",
      "dest": "server.admin.ts"
    },
    {
      "src": "/app/graphql",
      "dest": "server.app.ts"
    }
  ]
}
